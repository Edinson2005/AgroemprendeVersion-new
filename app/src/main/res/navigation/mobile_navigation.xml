<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.edinson.agroemnew.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_proyecto"
        android:name="com.edinson.agroemnew.ui.proyecto.ProyectoFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_proyecto" />

    <fragment
        android:id="@+id/navigation_perfil"
        android:name="com.edinson.agroemnew.ui.perfil.PerfilFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_perfil" />
    <fragment
        android:id="@+id/navigation_cerrar"
        android:name="com.edinson.agroemnew.ui.perfil.PerfilFragment"
        android:label="Cerrar Sesion"
        tools:layout="@layout/fragment_perfil" />
</navigation>